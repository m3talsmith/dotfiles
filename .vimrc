filetype plugin indent on
syntax on
set tabstop=2
set softtabstop=2
set shiftwidth=2
set expandtab
set smartindent
set showmatch
set guioptions-=T
set vb t_vb=
set ruler
set number
set incsearch
set fen
set fdl=0
set fdls=-1
set fcl=all
set fdo=block,hor,mark,percent,quickfix,search,tag,undo
set guifont=Ubuntu\ Mono:h20
set nofoldenable

colorscheme slate

au BufNewFile,BufRead *.ejs set filetype=html
